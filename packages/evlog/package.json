{
  "name": "evlog",
  "version": "1.8.0",
  "description": "Wide event logging library with structured error handling. Inspired by LoggingSucks.",
  "author": "HugoRCD <contact@hrcd.fr>",
  "homepage": "https://evlog.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/evlog.git"
  },
  "bugs": {
    "url": "https://github.com/HugoRCD/evlog/issues"
  },
  "keywords": [
    "logging",
    "wide-events",
    "structured-logging",
    "errors",
    "nuxt",
    "nitro",
    "typescript"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/module.d.mts",
      "import": "./dist/nuxt/module.mjs"
    },
    "./nitro": {
      "types": "./dist/nitro/module.d.mts",
      "import": "./dist/nitro/module.mjs"
    },
    "./nitro/v3": {
      "types": "./dist/nitro/v3/index.d.mts",
      "import": "./dist/nitro/v3/index.mjs"
    },
    "./workers": {
      "types": "./dist/workers.d.mts",
      "import": "./dist/workers.mjs"
    },
    "./axiom": {
      "types": "./dist/adapters/axiom.d.mts",
      "import": "./dist/adapters/axiom.mjs"
    },
    "./otlp": {
      "types": "./dist/adapters/otlp.d.mts",
      "import": "./dist/adapters/otlp.mjs"
    },
    "./posthog": {
      "types": "./dist/adapters/posthog.d.mts",
      "import": "./dist/adapters/posthog.mjs"
    },
    "./sentry": {
      "types": "./dist/adapters/sentry.d.mts",
      "import": "./dist/adapters/sentry.mjs"
    },
    "./better-stack": {
      "types": "./dist/adapters/better-stack.d.mts",
      "import": "./dist/adapters/better-stack.mjs"
    },
    "./enrichers": {
      "types": "./dist/enrichers.d.mts",
      "import": "./dist/enrichers.mjs"
    },
    "./pipeline": {
      "types": "./dist/pipeline.d.mts",
      "import": "./dist/pipeline.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.mts",
      "import": "./dist/browser.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "nuxt": [
        "./dist/nuxt/module.d.mts"
      ],
      "nitro": [
        "./dist/nitro/module.d.mts"
      ],
      "nitro/v3": [
        "./dist/nitro/v3/index.d.mts"
      ],
      "workers": [
        "./dist/workers.d.mts"
      ],
      "axiom": [
        "./dist/adapters/axiom.d.mts"
      ],
      "otlp": [
        "./dist/adapters/otlp.d.mts"
      ],
      "posthog": [
        "./dist/adapters/posthog.d.mts"
      ],
      "sentry": [
        "./dist/adapters/sentry.d.mts"
      ],
      "better-stack": [
        "./dist/adapters/better-stack.d.mts"
      ],
      "enrichers": [
        "./dist/enrichers.d.mts"
      ],
      "pipeline": [
        "./dist/pipeline.d.mts"
      ],
      "browser": [
        "./dist/browser.d.mts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "dev:prepare": "tsdown",
    "release": "bun run lint && bun run test && bun run build && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "echo 'Typecheck handled by build'"
  },
  "devDependencies": {
    "ufo": "^1.6.3",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/schema": "^4.3.1",
    "@nuxt/test-utils": "^4.0.0",
    "changelogen": "^0.6.2",
    "consola": "^3.4.2",
    "h3": "^1.15.5",
    "happy-dom": "^20.6.1",
    "nitro": "^3.0.1-alpha.2",
    "nitropack": "^2.13.1",
    "nuxt": "^4.3.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@nuxt/kit": "^4.3.1",
    "h3": "^1.15.5",
    "nitropack": "^2.13.1",
    "ofetch": "^1.5.1",
    "nitro": "^3.0.1-alpha.2"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "h3": {
      "optional": true
    },
    "nitropack": {
      "optional": true
    },
    "ofetch": {
      "optional": true
    },
    "nitro": {
      "optional": true
    }
  }
}
